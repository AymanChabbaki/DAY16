function matrixAddition(a, b){
  let res=[];
  for (let i = 0 ; i<a.length ; i++){
    for (let j = 0; j<b.length;j++){
        if (!res[i]) {
            res[i] = [];
        }
        res[i][j] = a[i][j]+b[i][j];
  }}
  return res
}

console.log(matrixAddition([[1,2],[3,4]], [[5,6],[7,8]])); // [[6,8],[10,12]]